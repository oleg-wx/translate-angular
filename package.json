{
  "name": "simply-translate-angular",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build translate",
    "build:prod": "ng build translate --configuration production",
    "build:prod:patch": "cd ./projects/translate && npm --no-git-tag-version version patch && cd ../.. && npm run build:prod",
    "publish:prod": "npm run build:prod && cd dist/translate && npm publish && cd ../..",
    "publish:prod:patch": "npm run build:prod:patch && cd dist/translate && npm publish && cd ../..",
    "build:demo": "ng build",
    "build:demo:prod": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng build --configuration production --aot",
    "start:demo": "ng serve --port 4800 --live-reload false",
    "restart:all": "npm run build:library:prod && npm run start:demo",
    "test": "ng test",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^13.1.2",
    "@angular/cdk": "^13.1.2",
    "@angular/cli": "^13.1.3",
    "@angular/common": "^13.1.2",
    "@angular/compiler": "^13.1.2",
    "@angular/compiler-cli": "^13.1.2",
    "@angular/core": "^13.1.2",
    "@angular/forms": "^13.1.2",
    "@angular/language-service": "^13.1.2",
    "@angular/material": "^13.1.2",
    "@angular/platform-browser": "^13.1.2",
    "@angular/platform-browser-dynamic": "^13.1.2",
    "@angular/router": "^13.1.2",
    "rxjs": "^7.3.0",
    "simply-translate": "^0.12.1",
    "tslib": "^2.3.1",
    "typescript": "4.5.4",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.1.3",
    "@types/jasmine": "~3.10.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "~17.0.8",
    "jasmine-core": "~4.0.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.11",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^13.1.3",
    "protractor": "~7.0.0",
    "tslint": "~6.1.3"
  }
}
