{
  "name": "simply-translate-angular",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build translate",
    "build:prod": "ng build translate --configuration production",
    "publish:prod": "npm run build:prod && cd dist/translate && npm publish && cd ../..",
    "build:demo": "ng build",
    "build:demo:prod": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng build --configuration production --aot",
    "start:demo": "ng serve --port 4800 --live-reload false",
    "restart:all": "npm run build:library:prod && npm run start:demo",
    "test": "ng test",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^12.2.0",
    "@angular/cdk": "^11.2.13",
    "@angular/cli": "^12.2.0",
    "@angular/common": "^12.2.0",
    "@angular/compiler": "^12.2.0",
    "@angular/compiler-cli": "^12.2.0",
    "@angular/core": "^12.2.0",
    "@angular/forms": "^12.2.0",
    "@angular/language-service": "^12.2.0",
    "@angular/material": "^11.2.13",
    "@angular/platform-browser": "^12.2.0",
    "@angular/platform-browser-dynamic": "^12.2.0",
    "@angular/router": "^12.2.0",
    "rxjs": "^7.3.0",
    "simply-translate": "^0.11.3",
    "tslib": "^2.2.0",
    "typescript": "4.3.5",
    "uuid": "^3.4.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.2.0",
    "protractor": "~7.0.0",
    "tslint": "~6.1.3"
  }
}
